{
  "name": "frontend-public-decisions",
  "version": "1.9.0",
  "private": true,
  "description": "Frontend of the Public Decisions application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lblod/frontend-public-decisions.git"
  },
  "license": "MIT",
  "author": "info@redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint --allow-empty-input \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:dependencies": "ember dependency-lint",
    "release": "release-it",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test"
  },
  "overrides": {
    "@ember/string": "^4.0.0"
  },
  "overridesComments": {
    "@ember/string": "power-select v7 depends on @ember/string v3 but we can't update to v8 yet because it depends on concurrency v4 (which not all our addons support yet)"
  },
  "devDependencies": {
    "@appuniversum/ember-appuniversum": "^3.7.0",
