{
  "name": "frontend-public-decisions",
  "version": "1.5.0",
  "private": true,
  "description": "Frontend of the Public Decisions application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lblod/frontend-public-decisions.git"
  },
  "license": "MIT",
  "author": "info@redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint --allow-empty-input \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:dependencies": "ember dependency-lint",
    "release": "release-it",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test"
  },
  "overrides": {
    "@ember/test-helpers": "$@ember/test-helpers",
    "@embroider/macros": "^1.0.0",
    "@zestia/ember-auto-focus": "5.1.0",
    "ember-modifier": "$ember-modifier",
    "torii": "1.0.0-beta.2"
  },
  "overridesNotes": {
    "@ember/test-helpers": "ember-file-upload v7 brings in an older version. We can drop the override once Appuniversum updates to v8",
    "@zestia/ember-auto-focus": "Appuniversum depends on v4 which pulls in some outdated packages",
    "ember-modifier": "Appuniversum brings in v3",
    "torii": "ember-acmidm-login v1 brings in a torii version that isn't compatible with Ember v4. Once we switch to v2 we can drop the override"
  },
  "devDependencies": {
    "@appuniversum/ember-appuniversum": "^2.18.0",
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.0.4",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@lblod/ember-acmidm-login": "^1.3.0",
    "@lblod/ember-mock-login": "^0.7.0",
    "@lblod/ember-submission-form-fields": "^2.11.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~5.4.1",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-display-revisions": "^2.1.2",
    "ember-cli-deploy-revision-data": "^2.0.0",
    "ember-cli-deploy-rsync": "0.0.5",
    "ember-cli-deploy-ssh-index": "^1.0.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^2.2.1",
    "ember-data": "~5.3.0",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-moment": "^10.0.0",
    "ember-mu-transform-helpers": "^2.0.0",
    "ember-plausible": "^0.1.0",
    "ember-power-select": "^7.2.0",
    "ember-promise-helpers": "^2.0.0",
    "ember-qunit": "^8.0.1",
    "ember-resolver": "^11.0.1",
    "ember-simple-auth": "^6.0.0",
    "ember-source": "~5.4.0",
    "ember-template-lint": "^5.11.2",
    "ember-truth-helpers": "^3.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-qunit": "^8.0.1",
    "loader.js": "^4.7.0",
    "lodash.camelcase": "4.3.0",
    "moment": "^2.29.4",
    "prettier": "^3.0.3",
    "qunit": "^2.20.0",
    "qunit-dom": "^2.0.0",
    "rdflib": "^2.2.19",
    "release-it": "^15.4.1",
    "sass": "^1.48.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2",
    "tracked-built-ins": "^3.3.0",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  }
}
